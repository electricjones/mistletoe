{
    "name": "electricjones/mistletoe",
    "type": "library",
    "description": "Cron and Single Use Task Management for PHP.",
    "keywords": [
        "cron",
        "tasks"
    ],
    "homepage": "https://github.com/electricjones/mistletoe",
    "license": "MIT",
    "authors": [
        {
            "name": "Michael Wilson",
            "email": "mwilson@electricjones.me",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=8.1",
        "symfony/console": "^6.0",
        "dragonmantank/cron-expression": "^3.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "bin": [
        "bin/mt"
    ],
    "autoload": {
        "psr-4": {
            "ElectricJones\\Mistletoe\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ElectricJones\\Mistletoe\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
